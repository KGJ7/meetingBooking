<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Register.RegisterController">
    <children>
        <Label layoutX="14.0" layoutY="46.0" text="Username:" AnchorPane.topAnchor="45.0" />
        <TextField fx:id="usernameField" layoutX="132.0" layoutY="41.0" prefHeight="25.0" prefWidth="190.0" promptText="Type username here" AnchorPane.topAnchor="41.0" />
        <Label layoutX="14.0" layoutY="84.0" text="First name:" AnchorPane.topAnchor="85.0" />
        <TextField fx:id="firstnameField" layoutX="132.0" layoutY="81.0" prefHeight="25.0" prefWidth="190.0" promptText="Type first name here" AnchorPane.topAnchor="81.0" />
        <Label layoutX="14.0" layoutY="121.0" text="Last name:" AnchorPane.topAnchor="125.0" />
        <TextField fx:id="lastnameField" layoutX="132.0" layoutY="121.0" prefHeight="25.0" prefWidth="190.0" promptText="Type last name here" />
        <Label layoutX="14.0" layoutY="162.0" text="Password:" AnchorPane.topAnchor="165.0" />
        <Label layoutX="14.0" layoutY="200.0" text="Retype password:" AnchorPane.topAnchor="205.0" />
        <PasswordField fx:id="passwordField" layoutX="132.0" layoutY="161.0" prefHeight="25.0" prefWidth="190.0" promptText="Type password here" />
        <PasswordField fx:id="confirmPasswordField" layoutX="132.0" layoutY="201.0" prefHeight="25.0" prefWidth="190.0" promptText="Retype password here" />
        <Label layoutX="14.0" layoutY="239.0" text="Email address:" AnchorPane.topAnchor="245.0" />
        <TextField fx:id="emailField" layoutX="132.0" layoutY="241.0" prefHeight="25.0" prefWidth="190.0" promptText="Type email address here" />
        <Button fx:id="cancelButton" layoutX="121.0" layoutY="321.0" mnemonicParsing="false" onAction="#goBackToLogin" text="Cancel" AnchorPane.leftAnchor="120.0" />
        <Button fx:id="registerButton" layoutX="210.0" layoutY="321.0" mnemonicParsing="false" onAction="#registerAdmin" text="Register" AnchorPane.rightAnchor="120.0" />
        <Label fx:id="errorLabel" layoutX="132.0" layoutY="276.0" prefHeight="17.0" prefWidth="190.0" />
    </children>
</AnchorPane>
