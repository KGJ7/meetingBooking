<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.LoginController">
   <children>
      <ImageView fitHeight="368.0" fitWidth="300.0" layoutX="286.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="loginButton" layoutX="62.0" layoutY="230.0" mnemonicParsing="false" onAction="#login" prefHeight="49.0" prefWidth="49.0" text="Log in" />
      <Button fx:id="registerButton" layoutX="56.0" layoutY="331.0" mnemonicParsing="false" onAction="#registerUser" text="Register" />
      <TextField fx:id="usernameField" layoutX="12.0" layoutY="111.0" promptText="Username" />
      <PasswordField fx:id="passwordField" layoutX="12.0" layoutY="143.0" promptText="Password" />
      <ComboBox fx:id="accountType" layoutX="11.0" layoutY="175.0" prefWidth="150.0" promptText="Account type" />
      <Text layoutX="68.0" layoutY="99.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign in" />
      <Text layoutX="54.0" layoutY="327.0" strokeType="OUTSIDE" strokeWidth="0.0" text="No account?" />
      <Label fx:id="connectionLabel" layoutX="11.0" layoutY="374.0" prefHeight="17.0" prefWidth="108.0" />
      <Label fx:id="meetingBookerLabel" layoutX="45.0" layoutY="47.0" text="Meeting booker" />
      <Label fx:id="credentialsLabel" layoutX="44.0" layoutY="206.0" prefHeight="17.0" prefWidth="85.0" />
   </children>
</AnchorPane>
